{
    "name": "PageBlocker",
    "version": "0.0.1",
    "manifest_version": 2,

    "description": "Blocks certain pages in certain parts of the day",
    "icons": { "128": "icon_128.png" },

    "author": "Scurgery",

    "browser_action": {
        "default_icon": "icon.png", 
        "default_popup": "popup.html"
    },

    "web_accessible_resources": [
        "urls.json"
    ],

    "permissions": ["<all_urls>"]
}